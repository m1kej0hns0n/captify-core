{
    "name": "@captify-io/core",
    "version": "1.0.3",
    "description": "Core components, types, and utilities for Captify platform",
    "main": "dist/components.mjs",
    "module": "dist/components.mjs",
    "types": "dist/components.d.mts",
    "exports": {
        ".": {
            "types": "./dist/components.d.mts",
            "default": "./dist/components.mjs"
        },
        "./services": {
            "types": "./dist/services.d.mts",
            "default": "./dist/services.mjs"
        },
        "./components": {
            "types": "./dist/components.d.mts",
            "default": "./dist/components.mjs"
        },
        "./hooks": {
            "types": "./dist/hooks.d.mts",
            "default": "./dist/hooks.mjs"
        },
        "./types": {
            "types": "./dist/types.d.mts",
            "default": "./dist/types.mjs"
        },
        "./api": {
            "types": "./dist/api.d.mts",
            "default": "./dist/api.mjs"
        },
        "./ui": {
            "types": "./dist/ui.d.mts",
            "default": "./dist/ui.mjs"
        },
        "./auth": {
            "types": "./dist/auth.d.mts",
            "default": "./dist/auth.mjs"
        }
    },
    "scripts": {
        "build": "tsup",
        "dev": "cross-env NODE_ENV=development tsup --watch",
        "type-check": "npx tsc --noEmit",
        "clean": "rimraf dist",
        "install-package": "node -r ts-node/register installer.ts"
    },
    "dependencies": {
        "@aws-sdk/client-cognito-identity": "^3.876.0",
