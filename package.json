    "files": [
        "dist",
        "README.md",
        "package.json"
    ],
    "devDependencies": {
        "@types/node": "^24.3.1",
        "@types/react": "^19.1.12",
        "@types/react-dom": "^19.1.9",
        "cross-env": "^7.0.3",
        "esbuild": "0.25.8",
        "rimraf": "^6.0.1",
        "ts-node": "^10.9.2",
        "tsup": "^8.5.0",
        "typescript": "^5.9.2"
    }
}
