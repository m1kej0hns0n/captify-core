        "input-otp": "^1.4.1",
        "lucide-react": "^0.542.0",
        "next": "15.5.2",
        "next-auth": "5.0.0-beta.29",
        "next-themes": "^0.4.6",
        "react": "19.1.1",
        "react-day-picker": "^9.0.0",
        "react-dom": "19.1.1",
        "react-hook-form": "^7.54.2",
